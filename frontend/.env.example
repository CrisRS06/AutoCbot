# ========================================
# Frontend Environment Variables Example
# ========================================
#
# Copy this file to .env.local for development
# For production, use .env.production
#

# Backend API URL (Development)
NEXT_PUBLIC_API_URL=http://localhost:8000

# WebSocket URL (Development)
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# App Configuration
NEXT_PUBLIC_APP_NAME=AutoCbot
NEXT_PUBLIC_APP_VERSION=1.0.0-dev

# Feature Flags (Development - All Enabled)
NEXT_PUBLIC_FEATURE_ENABLE_LIVE_TRADING=false
NEXT_PUBLIC_FEATURE_ENABLE_PAPER_TRADING=true
NEXT_PUBLIC_FEATURE_ENABLE_SMART_ORDERS=true
NEXT_PUBLIC_FEATURE_ENABLE_ML_STRATEGY=true
NEXT_PUBLIC_FEATURE_ENABLE_BACKTEST=true
NEXT_PUBLIC_FEATURE_ENABLE_STRATEGY_CREATION=true
NEXT_PUBLIC_FEATURE_ENABLE_ADVANCED_METRICS=true
NEXT_PUBLIC_FEATURE_ENABLE_PERFORMANCE_CHARTS=true
NEXT_PUBLIC_FEATURE_ENABLE_PNL_CHARTS=true
NEXT_PUBLIC_FEATURE_ENABLE_TELEGRAM=false
NEXT_PUBLIC_FEATURE_ENABLE_EMAIL_NOTIFICATIONS=false
NEXT_PUBLIC_FEATURE_ENABLE_WEBHOOKS=false
NEXT_PUBLIC_FEATURE_ENABLE_DARK_MODE=true
NEXT_PUBLIC_FEATURE_ENABLE_EXPORT=true
